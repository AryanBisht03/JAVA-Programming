package new_day;
import java.util.*;

// is-a relationship program----> it is just like simple inheritance and calling the parent methods.
class Animal
{
	public void animal()
	{
		System.out.println("This belogs to animal class");
	}
}

class Dog extends Animal
{
	public void dog()
	{
		System.out.println("hy i am dog");
	}
}

public class ISHAS {
public static void main(String args[])
{
	Dog d=new Dog();
	d.animal();
	d.dog();
}
}

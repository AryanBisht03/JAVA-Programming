package day_1;
import java.util.*;
public class result {
public static void main(String args[])
{
	Scanner scanner=new Scanner(System.in);
	int marks1;
	int marks2;
	int marks3;
	int marks4;
	int marks5;
	System.out.println("Enter your mark of sub1");
	marks1=scanner.nextInt();
	System.out.println("Enter your mark of sub2");
	marks2=scanner.nextInt();
	System.out.println("Enter your mark of sub3");
	marks3=scanner.nextInt();
	System.out.println("Enter your mark of sub4");
	marks4=scanner.nextInt();
	System.out.println("Enter your mark of sub5");
	marks5=scanner.nextInt();
	
	float percentage=((marks1+marks2+marks3+marks4+marks5)/500.0f)*100;
	System.out.println("The percentage is "+percentage);
	
	System.out.println();
	if(percentage>=75)
		System.out.println("DIstinction");
	else if(percentage>=45 && percentage<75)
		System.out.println("First class");
	else if(percentage>=33)
		System.out.println("Pass");
	else if(percentage<=33)
		System.out.println("Fail");
		
}
}

//2)	Create a class with static and non-static member variables.
//Define static and non-static member functions.
//Create instance of this class and call both static and non-static member functions.
package new_day;
class A
{
	private static int a=12;
	private int b;
	public A(int b)
	{
		this.b=b;
	}
	public int getb()
	{
		return b;
	}
	public static int geta()
	{
		return a;
	}
}
public class Inheri {
public static void main(String args[])
{
	A a=new A(12333);
	System.out.println("The value of public variable is "+a.getb());
	System.out.println("The value of atatic varaible is "+A.geta());
	System.out.println("The valiue of static variable by making an objecct is "+a.geta());
}
}

package new_day;
import java.util.*;

class Animal
{
	public void parent()
	{
		System.out.println("Hy im parent");
	}
}

class Dog extends Animal
{
	public void child()
	{
		System.out.println("HY im child");
	}
}

public class Upcasting {
public static void main(String args[])
{
	//upcasting
	//dog is treated like animal
	Animal a=new Dog();
	a.parent();

}
}
